     }}\n      >\n        {title}\n      </animated.div>\n      {/* Back */}\n      <animated.div\n        className=\"RatingsCard__back\"\n        style={{\n          opacity,\n          transform: transform.interpolate(inverseTransform)\n        }}\n      >\n        {/* Show rating only if the card is selected */}\n        {selected && (\n          <StarRating rating={currentRating} setRating={setRating} />\n        )}\n      </animated.div>\n    </animated.div>\n  );\n};\n\nexport default RatingsCard;\n","/Users/jordanmax/Desktop/ActiveProjects/2021-portfolio-react/src/components/star/index.js",[],{"ruleId":"103","severity":1,"message":"104","line":1,"column":8,"nodeType":"105","messageId":"106","endLine":1,"endColumn":12},{"ruleId":"103","severity":1,"message":"107","line":4,"column":10,"nodeType":"105","messageId":"106","endLine":4,"endColumn":18},{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":13,"column":23,"nodeType":"114","messageId":"115","endLine":13,"endColumn":25},{"ruleId":"112","severity":1,"message":"116","line":16,"column":22,"nodeType":"114","messageId":"115","endLine":16,"endColumn":24},{"ruleId":"112","severity":1,"message":"116","line":51,"column":24,"nodeType":"114","messageId":"115","endLine":51,"endColumn":26},{"ruleId":"112","severity":1,"message":"116","line":57,"column":29,"nodeType":"114","messageId":"115","endLine":57,"endColumn":31},{"ruleId":"117","severity":1,"message":"118","line":67,"column":21,"nodeType":"119","endLine":67,"endColumn":73},{"ruleId":"117","severity":1,"message":"118","line":82,"column":23,"nodeType":"119","endLine":91,"endColumn":25},{"ruleId":"108","replacedBy":"120"},{"ruleId":"110","replacedBy":"121"},{"ruleId":"103","severity":1,"message":"122","line":1,"column":17,"nodeType":"105","messageId":"106","endLine":1,"endColumn":26},{"ruleId":"103","severity":1,"message":"123","line":5,"column":10,"nodeType":"105","messageId":"106","endLine":5